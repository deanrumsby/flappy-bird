version 41
__lua__
-- flappy bird
-- by dean rumsby


waiting=true
game_over=false


-- factory functions --
-----------------------

function make_pipe(x,y)
 local pipe={
  x=x,
  y=y,
  w=24,
  h=96,
  dx=1,
  scored=false,
  
  update=function(self)
   self.x-=self.dx
  end,
  
  draw=function(self)
   -- top section
   spr(0,self.x,self.y,3,1)
   -- middle section
   for i=1,10 do
    spr(16,self.x,self.y+i*8,3,1)
   end
   -- bottom section
   spr(0,self.x,self.y+88,3,1,false,true)
  end
 }
 return pipe
end


-- score --
-----------

score={
 v=0,  -- score
 
 draw=function(self,x,y)
  local str=tostr(self.v)
  for i=1,#str do
   local d=sub(str,i,i)
   local x0=x-4*(#str-1)+8*(i-1)
   spr(32+d,x0,y,1,2)
  end
 end
}


-- game over heading --
-----------------------

go_head={
 x=35,
 y=23,
 
 draw=function(self)
  spr(160,self.x,self.y,8,2)
 end
}

-- game 0ver menu --
--------------------

go_menu={
 x=24,
 y=42,
 w=80,
 h=48,
 
 draw=function(self)
  local sx=self.w/8
  local sy=self.h/8
  -- corners
  spr(15,self.x,self.y)
  spr(15,self.x+self.w-9,self.y,1,1,true,false)
  spr(15,self.x+self.w-9,self.y+self.h-9,1,1,true,true)
  spr(15,self.x,self.y+self.h-9,1,1,false,true)
  -- top and bottom edge  
  for i=1,sx-2 do
   spr(47,self.x+i*8,self.y)
   spr(47,self.x+i*8,self.y+self.h-9,1,1,false,true)
  end
  -- left and right edge
  for i=1,sy-2 do
   spr(31,self.x,self.y+i*8,1,1,true,false)
   spr(31,self.x+self.w-9,self.y+i*8)
  end
  -- interior
  for i=1,sx-2 do
   for j=1,sy-2 do
    spr(127,self.x+i*8,self.y+j*8)
   end
  end
  -- score
  local score_x=self.x+self.w-30
  local score_y=self.y+7
  print("score",score_x,score_y,4)
  score:draw(score_x+12,score_y+8)
 end 
}
  

-- get ready heading --
-----------------------

get_ready={
 x=38,
 y=23,
 
 draw=function(self)
  spr(128,self.x,self.y,7,2)
 end
}


-- button prompt --
-------------------

button_prompt={
 x=56,
 y=45,
 
 draw=function(self)
  -- set grey scale
  pal({[8]=6,[9]=6,[10]=6})
  -- bird
  spr(4,self.x,self.y,2,2)
  spr(22,self.x,self.y+2)
  -- bounce lines
  spr(43,self.x-2,self.y+10)
  -- arrow
  spr(42,self.x+5,self.y+20)
  -- button
  spr(44,self.x+4,self.y+30)
  -- reset to normal colors
  pal()
  palt(0,false)
  palt(2,true)
  -- press banner
  spr(10,self.x+16,self.y+30,5,2)
 end
}


-- bird --
----------

bird={
 x=25,        -- x pos
 y=60,        -- y pos
 w=16,        -- width
 h=12,        -- height
 dy=0,        -- velocity
 wing_u=0,    -- wing u pos
 wing_v=3,    -- wing v pos
 flap=0,      -- animation counter
 a=0.5,       -- acceleration
 
 update=function(self)
  -- press üÖæÔ∏è to flap
  if btnp(üÖæÔ∏è) then
   self.dy=-4.5
   waiting=false
  end
  
  -- increment counter
  -- used for animation
  if self.flap<6 then
   self.flap+=1
  else
   self.flap=0
  end
  
  -- if waiting go no further
  if (waiting) return
  
  -- apply motion
  self.dy+=self.a
  self.y+=self.dy
 end,
 
 draw=function(self)
  -- body
  spr(4,self.x,self.y,2,2)
  -- wing animation
  local wing=7
  if (self.flap>=2) wing=6
  if (self.flap>=4) wing=22
  spr(wing,self.x+self.wing_u,self.y+self.wing_v)
 end
}


-- pipes --
-----------

pipes={
 list={}, -- array of pipes
 
 init=function(self)
  -- create initial gates
  for i=1,4 do
   add(self.list, self:add_gate(80+i*70))
  end
 end,
 
 add_gate=function(self, x)
  local y=rnd(54)+10 
  add(self.list,make_pipe(x,y-96))
  add(self.list,make_pipe(x,y+40))
 end,
 
 remove_gate=function(self)
  deli(self.list,1)
  deli(self.list,1)
 end, 
 
 update=function(self)
  -- update positions
  for p in all(self.list) do
   p:update()
  end
  
  -- only track visible 
  -- pipes
  local first=self.list[1]
  local last=self.list[#self.list]
  if first.x<-first.w then
   self:remove_gate()
   self:add_gate(last.x+70)
  end
 end,
  
 draw=function(self)
  for p in all(self.list) do
   p:draw()
  end
 end
}


-- ground --
------------

ground={
 y=114,  -- y pos
 c=-1,   -- animation counter
 
 update=function(self)
  -- update animation counter
  if self.c<7 then
   self.c+=1
  else
   self.c=0
  end
 end,
 
 draw=function(self)
  for i=0,19 do
    spr(9,i*8-self.c,self.y)
    spr(127,i*8,self.y+8)
  end
 end 
} 


-- game loop --
---------------

function _init()
 game_over=false
 -- wait for first input
 waiting=false
 
 -- change transparent color
 palt(0,false)
 palt(2,true)
 
 -- create starting pipes 
 pipes:init()
end

function _update()
 -- if game over do not update
 if (game_over) return
 
 bird:update()
 ground:update()

 -- if waiting do not update
 -- further
 if (waiting) return
 
 pipes:update()

 -- handle collisions
 resolve_collisions()
 
 check_score()
end

function _draw()
 -- clear screen
 cls()
 
 -- draw background image
 map()
 
 -- draw get ready sprites
 if (waiting) get_ready:draw()
 if (waiting) button_prompt:draw()
 
 -- draw entities
 pipes:draw()
 ground:draw()
 bird:draw()
 
 -- draw game over menu
 -- then stop drawing
 if game_over then
  go_head:draw()
  go_menu:draw()
  return
 end
 
 -- draw score in top
 -- center of screen
 score:draw(61,6)
end


-- scoring functions --
-----------------------

function check_score()
 local total=0
 -- each gate will score twice
 for i=1,#pipes.list do
  local p=pipes.list[i]
  if not p.scored 
  and p.x<bird.x then
   total+=1
   p.scored=true
  end
 end
 -- one point for one gate
 if total>=2 then
  score.v+=1
 end
end


-- collision system --
----------------------

-- using axis aligned
-- bounding box collision
function collision(a,b)
 return (
  a.x<b.x+b.w
  and a.x+a.w>b.x
  and a.y<b.y+b.h
  and a.y+a.h>b.y
 )
end

function resolve_collisions()
 -- bird and ground
 if bird.y+bird.h>=ground.y then
  game_over=true
  return
 end
 -- bird and pipe
 for p in all(pipes.list) do
  if collision(bird,p) then
   game_over=true
   return
  end
 end
end
